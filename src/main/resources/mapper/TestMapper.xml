<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.thymeleaf_template.mapper.TestMapper">
    <select id="selectNow" resultType="string">
        SELECT TO_CHAR(NOW(), 'YYYY-MM-DD HH24:MI:SS')
    </select>

    <select id="selectParam" parameterType="map" resultType="map">
        SELECT #{param1} as IN_PARAM1
             , #{param2} as IN_PARAM2
    </select>

    <select id="selectParamList" parameterType="map" resultType="map">
        SELECT #{param1} as IN_PARAM1
             , #{param2} as IN_PARAM2
        UNION ALL
        SELECT #{param1} as IN_PARAM1
             , #{param2} as IN_PARAM2
    </select>
</mapper>
